     "execution_count": 35,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Select second element of each the three dimensions\n",
    "\n",
    "tensor[1]\n",
    "\n",
    "tensor[1,1]\n",
    "\n",
    "tensor[1,1,1]\n",
    "\n",
    "tensor[1,1,1,1]"
   ]
  }
